{
	"extends": "./tsconfig.json",
	"compilerOptions": {
		"declaration": true,
		"moduleResolution": "node",
		"declarationMap": true,
		"emitDeclarationOnly": true,
		"outDir": "dist",
		"skipLibCheck": true,
		"noEmit": false,
		"downlevelIteration": true,
		"composite": false
	},
	"include": ["core/**/*.ts", "core/**/*.vue"]
}
